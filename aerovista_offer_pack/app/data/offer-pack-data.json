  "sales": {
    "pitch": "We deliver fast, premium, and practical work across web, interactive experiences, audio, training, design, publishing, and aerial media — built to ship quickly now, and scale later without rebuilding.",
    "qualification": [
      "1) What do you sell, and who’s the buyer?",
      "2) What’s the #1 action you want people to take?",
      "3) What do you already have? (domain/logo/photos/copy)",
      "4) What’s your deadline and why?",
      "5) What’s your “good outcome” in one sentence?"
    ],
    "objections": [
      "“I can do it myself”: You can — you’re buying time, polish, and a future-proof base.",
      "“Too expensive”: Let’s match scope to your outcome; we can launch small and expand.",
      "“Need it ASAP”: If assets are ready, we can move fast. Otherwise we start with a sprint.",
      "“Not sure what I need”: We’ll recommend the smallest package that gets you a win."
    ],
    "closePattern": [
      "Confirm outcome → choose package → confirm assets → confirm timeline → collect deposit → send intake."
    ],
    "followUp": [
      {
        "label": "Same day",
        "subject": "AeroVista — next step + intake",
        "body": "Here’s the intake form. Once we have assets and the deposit, we schedule immediately and start the sprint."
      },
      {
        "label": "48 hours",
        "subject": "Quick check-in",
        "body": "Want to proceed with the package we discussed or adjust scope? Either way I can keep momentum moving."
      }
    ]
  },
  "delivery": {
    "phases": [
      "1) Intake + deposit + assets",
      "2) Direction lock (scope, timeline, deliverables)",
      "3) Build/produce",
      "4) Review round 1",
      "5) Review round 2 (standard final)",
      "6) Delivery + handoff + archive"
    ],
    "revisionPolicy": [
      "- Two rounds included.",
      "- Additional changes become a small add-on or a change order."
    ],
    "handoff": [
      "Final deliverables",
      "“What changed” summary",
      "Maintenance notes / SOP",
      "Any credentials/access returned to client control"
    ],
    "qaChecklists": [
      {
        "id": "web",
        "name": "Web (LaunchPad / Quest Page / Mini-Game)",
        "items": [
          "Mobile layout passes",
          "Performance acceptable",
          "Contact flow tested",
          "Analytics (if included) verified",
          "Domain + SSL verified",
          "CTA visible above the fold",
          "Basic SEO fields present",
          "Accessibility quick pass (contrast, labels, focus)"
        ]
      },
      {
        "id": "audio",
        "name": "Audio (Sonic Logo / Podcast Pack / Track Polish)",
        "items": [
          "Loudness target consistent",
          "No clipping",
          "Stems exported (if included)",
          "File naming standard",
          "Client preview link works"
        ]
      },
      {
        "id": "design",
        "name": "Design (Brand Kit / Social Pack / Print)",
        "items": [
          "Export sizes correct",
          "Fonts licensed/converted properly",
          "Print bleed/safe zones checked (if applicable)",
          "Source files packaged"
        ]
      },
      {
        "id": "drone",
        "name": "Drone (Weather dependent)",
        "items": [
          "Flight plan + safety checks done",
          "Shots match brief angles",
          "Edits consistent and labeled",
          "Deliverables organized for client"
        ]
      }
    ]
  },
